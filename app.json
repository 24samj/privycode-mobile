{
  "expo": {
    "name": "PrivyCode",
    "slug": "privycode-mobile",
    "scheme": "privycode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["applinks:privycode.com"],
      "bundleIdentifier": "com.samtastic.privycodemobile",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "privycode",
            "CFBundleURLSchemes": ["privycode"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.samtastic.privycodemobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "privycode.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "privycode"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": ["expo-secure-store", "expo-font", "expo-web-browser"],
    "owner": "samj24"
  }
}
